{
  "name": "codejam-canvas",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  },
  "scripts": {
    "watch": "node-sass sass/main.scss style.css -w",
    "compile": "node-sass sass/main.scss css/style-comp.css",
    "prefix": "postcss --use autoprefixer -b \"last 5 versions\" css/style-comp.css -o css/style-prefix.css",
    "compress": "node-sass css/style-prefix.css style.css --output-style compressed",
    "build": "npm-run-all compile prefix compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LighthouseKeeperYN/codejam-canvas.git"
  },
  "author": "Anton Saladounikau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LighthouseKeeperYN/codejam-canvas/issues"
  },
  "homepage": "https://github.com/LighthouseKeeperYN/codejam-canvas#readme"
}
